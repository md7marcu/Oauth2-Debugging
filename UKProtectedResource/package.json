{
  "name": "UKProtectedResource",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && cp ../UKAuth/config/cert.pem config/serverCert.pem",
    "dev": "env-cmd -f ./config/.env ts-node-dev --inspect=0.0.0.0:9227 -- ./lib/server.ts",
    "start": "env-cmd -f ./config/.env nodemon ./dist/server.js",
    "prod": "env-cmd -f ./config/.env npm run build && npm run start",
    "test": "mocha -r ts-node/register test/**/*.spec.ts",
    "test-dev": "nodemon --watch . --ext ts --exec \"mocha -r ts-node/register test/**/*.spec.ts\"",
    "coverage": "nyc -r lcov -e .ts -x \"*.spec.ts\" mocha -r ts-node/register test/**/*.spec.ts",
    "postinstall": "node-config-ts",
    "watch": "tsc -w -p .",
    "map-debug": "tsc && env-cmd -f ./config/.env nodemon --legacy-watch --watch ./dist --inspect=0.0.0.0:9229 --nolazy ./dist/server.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "env-cmd": "^10.0.1",
    "express": "^4.17.1",
    "http-status-code": "^2.1.0",
    "http-status-codes": "^1.3.2",
    "lodash": "^4.17.15",
    "mongoose": "^5.7.5",
    "node-config-ts": "^3.0.4",
    "node-forge": "^0.9.1",
    "nodemon": "^1.19.3",
    "nyc": "^15.0.1",
    "path": "^0.12.7",
    "typescript": "^3.8.3",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "@types/assert": "^1.4.6",
    "@types/chai": "^4.2.11",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/node-forge": "^0.9.3",
    "@types/supertest": "^2.0.8",
    "assert": "^2.0.0",
    "chai": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.1.1",
    "source-map-support": "^0.5.19",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.2",
    "ts-node-dev": "^1.0.0-pre.43",
    "ts-sinon": "^1.2.0"
  }
}
