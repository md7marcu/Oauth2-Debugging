{    
    "issuer": "authorize.ultrakompis.com",
    "audience": "api.ultrakompis.com",
    "subject": "ultrakompis",
    "algorithm": "RS256",
    "authorizationEndpoint": "https://localhost:3001/authorize",
    "accessTokenEndpoint": "https://localhost:3001/token",
    "aliveEndpoint": "https://localhost:3001/alive",
    "scopes": "weight",
    "verifyRedirectUrl": true,
    "verifyClientId": true,
    "verifyScope": true,
    "verifyCode": true,
    "validateScope": true,
    "clearAuthorizationCode": true,
    "clearRequestId": true,
    "authorizationCodeLength": 16,
    "accessTokenLength": 16,
    "refreshTokenLength": 64,
    "clients": [
        {
            "clientId": "ukauth-client",
            "clientSecret": "secretsecretsecret",
            "redirectUris": ["http://localhost:3001/callback"],
            "scopes": ["weight", "something", "else"]
        }
    ],
    "expiryTime": 3600,
    "createdTimeAgo": 30,
    "addNonceToAccessToken": true,
    "saveAccessToken": true
}
